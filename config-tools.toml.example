[tools]
builtin = ["tavily_search"]  # 启用内置的tavily搜索工具
# 工具有analyze_image,code_runner, divination, create_art, get_github_trending, get_time, get_weather_data, jina_fact_checking, jina_reader, jina_search, picture_api, web_api
enabled = ["get_time"]  # 启用外部工具列表

[tavily]
api_key = ""  # 在此填入你的Tavily API密钥
max_results = 5  # 搜索结果最大数量

[img_analysis]
new_api_key = ""  # 在此填入图像分析服务的API密钥
new_base_url = ""  # 图像分析服务的基础URL
model = "gemini-1.5-pro-latest"  # 使用的模型名称
img_folder = "img"  # 图像文件存储目录

[openweather]
api_key = ""  # 在此填入OpenWeather API密钥

[jina]
api_key = ""  # 在此填入Jina API密钥
top_n = 30  # 返回结果的最大数量
min_length = 10  # 最小文本长度要求

[divination]
api_key = ""  # 在此填入占卜服务的API密钥
base_url = ""  # 占卜服务的基础URL
model = "llama-3.3-70b-instruct"  # 使用的模型名称

[create_art]
fal_key = ""  # 在此填入FAL服务的API密钥
openai_api_key = ""  # 在此填入OpenAI API密钥
openai_base_url = ""  # OpenAI服务的基础URL
model = "gemini-1.5-pro-latest"  # 使用的模型名称

[code_runner]
judge0_url = ""  # Judge0服务的URL
judge0_api_key = ""  # 在此填入Judge0 API密钥
cpu_time_limit = 5  # CPU时间限制
wall_time_limit = 8  # 总时间限制

[picture_api]
api = "" # 在此填入请求接口,例如:"https://picture.netqianye.com/api.php?type=" type设定:触发工具AI将匹配用户输入,例如:"动漫图片"则type=动漫


[memos]
url = "https://xxx.xxx.xx" #memos地址
memos_token = "xxxxx" # memos tokens
default_visibility = "PRIVATE" # memos可用查看的范围 PRIVATE:仅可见当前tokens所属memos， PUBLIC可查看所有公开的memos
page_size = 10 # 最大返回数量
user_id = 6 # 账户的id
